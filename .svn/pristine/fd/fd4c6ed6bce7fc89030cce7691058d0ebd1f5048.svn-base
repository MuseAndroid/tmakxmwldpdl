ul.cho_st li table td.end{padding-bottom:0;}
.title_f{background:#f8f8f8; font-size:16px; font-weight:bold; color:#0f547d; padding:12px; border-bottom:1px solid #dbdada;}
.memo_txt{padding:12px; color:#888; font-size:16px; line-height:20px;}

table.client_list{width:100%;}
table.client_list th{padding:10px 0; border-bottom:1px solid #ddd; font-size:0.813em; color:#333; width:100px;}
table.client_list th img{border-radius:50%; width:75px; height:75px;}
