@charset "utf-8";

/* CSS Document */

html {width:100%; margin:0; padding:0; }
body {min-width:320px; width:100%; font-weight:normal; font-size:0.938em; margin:0; padding:0; color:#666; line-height:15px; font-family:'Noto Sans KR', sans-serif; background-color: #e8eff7;}
@import url(http://fonts.googleapis.com/earlyaccess/notosanskr.css);
div, p, ol, ul, li, img, a, h1, h2, h3, h4, h5, h6, form, span, iframe, dl, dt, dd{margin:0; padding:0;}
table, th, td { border-collapse: collapse; margin:0; padding:0;}
table caption{display:none;}
input{vertical-align:middle;}
ul, ol, li, dt, dd, dl{list-style:none;}
select{vertical-align:middle; padding:0; margin:0;}
img, button{border:0;}
a:link, a:hover, a:active{text-decoration:none;}
a{text-decoration:none;text-overflow:ellipsis;-o-text-overflow:ellipsis;}
em{font-style:normal;}

/*=============================Layout===========================*/
/*body{/*background:#e8eff7;}*/
.app_title{position:relative; /*height:4.750em;*/height: auto; width:100%; top:0; background:#419ef7; z-index:10; overflow:hidden; display:inline-block; border-bottom:1px solid #2e87bc;}
.Header_m{position:fixed; height:2.750em; width:100%; top:0; background:#fff; z-index:10; overflow:hidden; display:inline-block; border-bottom:1px solid #e0e0e0;}

.white{background:#fff;}


/*----------Header------------*/
.app_title h1{position:relative; text-align:center; width:100%; top:0; font-size:24px; color:#fff;margin: 20px 0  4px 0;
  font-weight: 600;}
.Header h2{position:relative; text-align:left; top:0; font-size:20px; color:#fff; padding-top:0.750em; padding-left:0.5em; font-weight:normal;}
.Header div.menu{position:absolute; width:2.625em; height:2.625em; top:0; right:0.5em; background-position:0 0; cursor:pointer;}
.Header div.menu:hover{background-position:-42px 0;}
.Header a.home{position:absolute; width:2.625em; height:2.625em; top:0; left:0.5em; background-position:-84px 0;}
.Header a.home:hover{background-position:-126px 0;}
.Header a.close{position:absolute; width:2.625em; height:2.625em; top:0; right:0.5em; background-position:-168px 0;}
.Header a.close:hover{background-position:-210px 0;}
.Header a.back{position:absolute; width:2.625em; height:2.625em; top:0; left:0.5em; background-position:-84px -42px;}
.Header a.back:hover{background-position:-126px -42px;}

.Header_m h1{position:relative; text-align:center; width:100%; top:0;}
.Header_m h1 a.m_logo{width:66px; height:44px;  background:url(../img/merp.png) center center no-repeat; background-size:66px 44px; overflow:hidden; vertical-align:top; display:inline-block;}
.Header_m a.menu{position:absolute; width:2.625em; height:2.625em; top:0; right:0.5em; background-position:0 0;}
.Header_m a.menu:hover{background-position:-42px 0;}

/*2Depth*/
.Sub_menu{margin-top:2.750em; background:#fff; height:2.750em; border-bottom:2px solid #399bd6; text-align:center;}
.Sub_menu ul{padding-top:0.938em;}
.Sub_menu ul li{color:#3a3a3a; font-size:1.125em; margin-right:12%; display:inline-block; cursor:pointer;}
.Sub_menu ul li:hover{color:#399bd6;}
.Sub_menu ul li.sel{color:#399bd6;}
.Sub_menu ul li.end{margin-right:0;}


/*----------Index------------*/
.category{padding:1.875em 1.250em; margin-top:45px; text-align:center;}
.category ul{}
.category ul li{width:33%; text-align:center; float:left; color:#7d7e7e; font-size:0.813em; margin-bottom:2.625em; letter-spacing:-1px;}
.category ul li a{color:#7d7e7e;}
.category ul li a:hover{color:#179fec;}
.category ul li span{display:inline-block; width:55px; height:55px; margin-bottom:0.8em;}
.category ul li span.merp_01{background-position:0 0;}
.category ul li span.merp_02{background-position:-70px 0;}
.category ul li span.merp_03{background-position:-140px 0;}
.category ul li span.merp_04{background-position:-0 -70px;}
.category ul li span.merp_05{background-position:-70px -70px;}
.category ul li span.merp_06{background-position:-140px -70px;}
.category ul li span.merp_07{background-position:0 -140px;}
.category ul li span.merp_08{background-position:-70px -140px;}
.category ul li span.merp_09{background-position:-140px -140px;}
.category ul li.logo{width:100%; margin-bottom:60px;}

.banner{height:50px; text-align:center; width:100%; position:fixed; bottom:0; background:#666666; }

/*----------l_nav------------*/
.l_nav {background:#555;}

/*----------Search------------*/
.search{padding:0.5em 0.625em; margin-top:2.750em; border-bottom:1px solid #a2a2a2; background:#ffffff;}
.search span{ color:#333; font-size:1.250em;}
.search div.btn{position:absolute; width:2.625em; height:2.625em; top:0; right:0.5em; background-position:-252px 0; z-index:10; margin-top:4px;}
.search div.btn:hover{background-position:-294px 0;}

.search2{background:#ecf0f1; padding:0.5em 0; margin-top:0em; border-bottom:1px solid #b8d1df;}
.schall{margin:0 100px 0 8px; height:35px; border:1px solid #4d87ef; border-right:none; padding:7px 9px 0 9px;
		border-top-left-radiua:2px; border-bottom-left-radiua:2px; background-color:#fff;}

.search3{height:31px; margin-top:2.750em;padding-left:8px; padding-top:14px; border-bottom:1px solid #a2a2a2; background:#ffffff;}
.search3 span{ color:#333; font-size:18px; font-weight:bold;}
.search3 div.btn{position:absolute; width:2.625em; height:2.625em; top:0; right:0.5em; background-position:-252px 0; z-index:10; margin-top:4px;}
.search3 div.btn:hover{background-position:-294px 0;}

		
/*--Detail Search--*/
.detail{margin:0px; background:#fff; border:1px solid #ddd; font-size:15px; color:#7d7d7d;}
.detail ul{margin:20px;}
.detail ul li{display:inline-block; margin-right:20px; margin-bottom:7px;}
.detail ul li.mid{margin:15px 0 7px 0; width:100%;}
.detail ul li.end{width:100%;}
.detail ul li.cal{width:100%; margin:15px 0;}
.d_line{height:1px; border-top:1px solid #ddd;}
.t_line{border:1px solid #ddd; padding:7px;}
.t_line2{border:1px solid #ddd; padding:7px; width:80px; display:inline-block;}

/*-------------Client------------*/
